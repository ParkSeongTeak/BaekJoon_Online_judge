#include<iostream>
#include<algorithm>
#include<queue>
#include<stack>
using namespace std;

//뭘 원하는지는 알지만 굳이? 반드시 연결 가능하다는 조건이 있는데?


int main() {
	int T;
	int N, M, p1, p2;

	cin >> T;
	for (int t = 0;t < T;t++) {

		cin >> N >> M;

		for (int i = 0;i < M;i++) {
			cin >> p1 >> p2;
		}
		cout << N - 1<<"\n";
	}



}
