#include<iostream>
#include<algorithm>
#include<queue>
using namespace std;

int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);

	priority_queue<int> PQ;
	int N;
	cin >> N;
	for (int i = 0; i < N; i++) {
		int tmp;
		cin >> tmp;
		if (tmp > 0) {
			PQ.push(tmp);
		}
		else {
			if (PQ.empty()) {
				cout << 0 << "\n";
			}
			else {
				cout << PQ.top() << "\n";
				PQ.pop();
			}
		}
	}
}
