#include<iostream>
#include<vector>
#include<algorithm>
#include<queue>
using namespace std;

class procession {
	int y, x;
	
	
public :
	int num;
	procession(int Y = 0, int X = 0,int idx = 0) :y(Y), x(X), num(idx) {};

	bool operator > (procession& other) {
		return this->x > other.x;
	}

	
};


int main() {
	
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);

	int N, r, c; //(1,500)
	//항상 순서대로 곱셈 할 수 있는 크기만 준다. 

	vector<procession> arr;
	vector<procession> Forsort;
	vector<int>table;
	
	cin >> N;
	for (int i = 0; i < N; i++) {
		int y,x;
		cin >> y >> x;
		procession tmp(y, x, i);
		arr.push_back(tmp);
		Forsort.push_back(tmp);
	}
	sort(Forsort.begin(), Forsort.end()- sizeof(procession));

	for (int i = 0; i < Forsort.size(); i++) {
		table.push_back(Forsort[i].num);
	}





}
