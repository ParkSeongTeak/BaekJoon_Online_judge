#include <iostream>
#include <algorithm>
#include <vector>
using namespace std;

int n;
//int idx = 0;
int tmp1,tmp2;
vector <int> num;
int Operator[4];	// + - * /
int Min = 1987654321;
int Max = -1987654321;
void calcul(int idx , int ans) {
	if (n-1 == idx) {
		if (ans < Min)
			Min = ans;
		if (ans > Max)
			Max = ans;

		return;
	}

	for (int i = 0; i < 4; i++) {
		if (Operator[i] > 0) {
			switch(i)
			{case 0: 
				ans += num[idx+1];
				Operator[i] -= 1;
				calcul(idx + 1, ans);
				Operator[i] += 1;
				ans -= num[idx+1];
				break;
			case 1:
				ans -= num[idx+1];
				Operator[i] -= 1;
				calcul(idx + 1, ans);
				Operator[i] += 1;
				ans += num[idx+1];
				break;

			case 2:
				tmp1 = ans;
				ans *= num[idx+1];
				Operator[i] -= 1;
				calcul(idx + 1, ans);
				Operator[i] += 1;
				ans = tmp1;
				break;

			case 3:
				tmp2 = ans;

				if (ans < 0 && num[idx + 1]>0) {
					ans =(-1*ans) / num[idx + 1];
					ans = (-1 * ans);

				}
				else{
					ans /= num[idx + 1];
				}
				Operator[i] -= 1;
				calcul(idx + 1, ans);
				Operator[i] += 1;
				ans = tmp2;
				break;


			}
		}
	}

}

int main() {
	cin >> n;
	int tmp;
	for (int i = 0; i < n; i++) {
		cin >> tmp;
		num.push_back(tmp);
	}
	for (int i = 0; i < 4; i++) {
		cin >> Operator[i];
	}
	
	calcul(0, num[0]);
	cout << Max<<"\n" << Min;
}
